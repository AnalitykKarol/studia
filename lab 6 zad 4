#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n;
    cout << "Podaj rozmiar tablicy: ";
    cin >> n;

    vector<double> array(n);

    cout << "Podaj elementy tablicy: ";
    for (int i = 0; i < n; ++i) {
        cin >> array[i];
    }

    vector<double> progress;
    vector<double> biggie;

    for (int i = 0; i < n; ++i) {
        if (array[i] > 0) {
            // Jeśli liczba jest dodatnia, dodaj ją do bieżącego podciągu
            progress.push_back(array[i]);
        } else {
            // Jeśli liczba nie jest dodatnia, zakończ bieżący podciąg
            if (progress.size() > biggie.size()) {
                biggie = progress;
            }
            progress.clear();
        }
    }

    if (progress.size() > biggie.size()) {
        biggie = progress;
    }

    cout << "Najdluzszy podciag liczb dodatnich:" << endl;
    for (double num : biggie) {
        cout << num << ", ";
    }

    return 0;
}
